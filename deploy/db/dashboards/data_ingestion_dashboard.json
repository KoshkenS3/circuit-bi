          "rawSql": "with max_block_number_by_chain as materialized (\n  select \n    p.chain, \n    max((import_data->'data'->>'chainLatestBlockNumber')::integer) as last_covered\n  from import_status i\n  join product p on p.product_id = i.product_id\n  group by 1\n)\nselect \n  product_key, \n  i.import_data is not null as has_import_status,\n  (import_data->'data'->>'contractCreatedAtBlock')::integer as contract_creation_block,\n  (import_data->'data'->>'chainLatestBlockNumber')::integer as latest_covered_block_number,\n  b.last_covered as last_block_number_covered_for_chain,\n  b.last_covered - coalesce((import_data->'data'->>'chainLatestBlockNumber')::integer, (import_data->'data'->>'contractCreatedAtBlock')::integer) as blocks_to_cover\nfrom product p\nleft join import_status i on p.product_id = i.product_id\nleft join max_block_number_by_chain b on p.chain = b.chain",
          "refId": "A",
          "select": [[{ "params": ["value"], "type": "column" }]],
