          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "\nwith balance_gf_ts as (\n  SELECT\n    time_bucket_gapfill('$interval', datetime) as datetime,\n    product_id,\n    locf(last(balance::numeric, datetime)) as balance\n  from investment_balance_ts\n  WHERE\n    $__timeFilter(datetime)\n    and investor_id = $investor_id\n  GROUP BY 1, 2\n),\nbalance_ts as (\n  select * from balance_gf_ts where balance is not null\n),\nprice_gf_ts as (\n  select\n    time_bucket_gapfill('$interval', datetime) as datetime,\n    price_feed_id as price_feed_id,\n    locf(last(usd_value::numeric, datetime)) as usd_value\n  from asset_price_ts\n  where \n    $__timeFilter(datetime)\n    and price_feed_id in ($price_feed_ids)\n  group by 1,2\n),\nprice_ts as (\n  select * from price_gf_ts where usd_value is not null\n)\nselect\n  b.datetime as time,\n  pr.product_key,\n  (b.balance * p.usd_value) as \"$\"\nfrom balance_ts b\n  left join product pr on b.product_id = pr.product_id\n  left join price_ts p on b.datetime = p.datetime and pr.price_feed_id = p.price_feed_id\norder by 1;",
